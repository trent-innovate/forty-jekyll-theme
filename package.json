{
  "name": "jekyll-vue-template",
  "version": "2.0.0",
  "description": "A starter template for Jekyll projects with Vue.js and Vue Single File Components, complete with webpack.",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "jekyll:watch": "bundle exec jekyll build --watch --incremental",
    "jekyll:build": "bundle exec jekyll build",
    "serve": "browser-sync start --config browser-sync.js",
    "start": "npm run jekyll:watch & npm run serve",
    "watch": "cross-env NODE_ENV=development webpack --watch"
  },
  "keywords": [
    "jekyll",
    "vue",
    "ssg"
  ],
  "author": "Christopher Murphy",
  "license": "MIT",
  "dependencies": {
    "particles.js": "^2.0.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  }
}
